<div class="jumbotron">
        <div class="row justify-content-md-center">
          <div class="col-4-text-nowrap"><i class="fa fa-fw fa-server" aria-hidden="true"></i> Members:</div>
          <div class="col-5-text-nowrap"><strong><%= guild.members.size %> Members</strong></div>
        </div>
        <div class="row justify-content-md-center">
          <div class="col-4-text-nowrap"><i class="fa fa-fw fa-clock-o" aria-hidden="true"></i> Guild ID:</div>
          <div class="col-5-text-nowrap"><strong><%= guild.id %></strong></div>
        </div>
